{
    "name": "z-golly/opcache",
    "description": "php opcache",
    "license": "MIT",
    "authors": [
        {
            "name": "jonathan",
            "email": "jianyang1209@outlook.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^7.0|^8.0",
        "symfony/finder": "^5.2"
    },
    "require-dev": {
        "ext-zend-opcache": "*"
    },
    "autoload": {
        "psr-4": {
            "Golly\\Opcache\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Golly\\Opcache\\OpcacheServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
